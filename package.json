{
  "name": "react-snowpack-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:frontend && npm run build:backend",
    "build:backend": "tsc -p ./tsconfig.server.json",
    "build:frontend": "snowpack build",
    "dev": "cross-env TS_NODE_PROJECT='./tsconfig.server.json' NODE_ENV=development concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cross-env NODE_ENV=development nodemon --config nodemon.json server/index.ts",
    "dev:frontend": "cross-env NODE_ENV=development snowpack dev",
    "start": "node ./dist"
  },
  "devDependencies": {
    "@snowpack/plugin-dotenv": "^2.0.5",
    "@snowpack/plugin-react-refresh": "^2.4.1",
    "@snowpack/plugin-run-script": "^2.3.0",
    "@snowpack/plugin-sass": "^1.4.0",
    "@snowpack/plugin-typescript": "^1.2.1",
    "@types/express": "^4.17.11",
    "@types/node-sass": "^4.11.1",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "concurrently": "^6.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.1.3",
    "eslint": "^7.22.0",
    "eslint-watch": "^7.0.0",
    "http-proxy": "^1.18.1",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "snowpack": "^3.1.2",
    "snowpack-plugin-relative-css-urls": "^1.1.1",
    "style-loader": "^2.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "axios": "^0.21.1",
    "express": "^4.17.1"
  }
}
